{
	"name": "minimap-app",
	"private": true,
	"scripts": {
		"tauri": "tauri",
		"tauri:build": "tauri build",
		"tauri:dev": "tauri dev",
		"dev": "npx serve -l 3891 --no-port-switching -n -S build",
		"format": "prettier --write .",
		"lint": "prettier --lint .",
		"build": "set MM_DEVELOPMENT=1&&set MM_LOCAL=1&&gulp",
		"build:prod": "gulp",
		"build:prod:local": "set MM_LOCAL=1&&gulp",
		"build:pp": "node ./script/preprocessor.mjs --lang-file=./package/web/i18n.tsv --tag=I \"./package/web/js/**/*.mjs\""
	},
	"dependencies": {
		"minimap": "./www-src"
	},
	"devDependencies": {
		"@tauri-apps/cli": "1.5.6",
		"acorn": "7.4.1",
		"acorn-jsx": "5.3.1",
		"acorn-jsx-walk": "2.0.0",
		"acorn-walk": "8.1.0",
		"arg": "5.0.0",
		"camel-case": "4.1.2",
		"chalk": "4.1.1",
		"css": "2.2.4",
		"css-what": "3.4.2",
		"globby": "11.0.3",
		"gulp": "4.0.2",
		"gulp-esbuild": "0.8.2",
		"gulp-htmlnano": "0.1.0",
		"gulp-multi-process": "1.4.0",
		"gulp-nodemon": "2.5.0",
		"gulp-replace": "1.1.3",
		"gulp-run": "1.7.1",
		"gulp-to-ico": "1.0.0",
		"htmlentities": "1.0.0",
		"p-limit": "4.0.0",
		"prettier": "2.2.1",
		"surplus": "0.5.3",
		"through2": "3.0.1",
		"vinyl": "2.2.1"
	},
	"prettier": {
		"useTabs": true,
		"semi": true,
		"singleQuote": true,
		"jsxSingleQuote": false,
		"trailingComma": "none",
		"jsxBracketSameLine": false,
		"arrowParens": "avoid",
		"requirePragma": false,
		"insertPragma": false,
		"endOfLine": "lf"
	}
}
